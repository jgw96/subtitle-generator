{
  "name": "pwa-starter",
  "version": "0.0.1",
  "description": "A starter kit for building PWAs!",
  "main": "index.js",
  "scripts": {
    "dev-server": "vite --open",
    "dev": "npm run dev-server",
    "dev-task": "vite",
    "deploy": "npx @azure/static-web-apps-cli login --no-use-keychain && npx @azure/static-web-apps-cli deploy",
    "build": "tsc && vite build",
    "start": "npm run dev",
    "start-remote": "vite --host"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fluentui/web-components": "^2.5.17",
    "@shoelace-style/shoelace": "^2.14.0",
    "@thepassle/app-tools": "^0.9.12",
    "@xenova/transformers": "^2.17.0",
    "lit": "^3.1.2",
    "speech-to-text-toolkit": "^0.0.7",
    "urlpattern-polyfill": "^10.0.0",
    "workbox-build": "^6.5.2",
    "workbox-core": "^6.5.2",
    "workbox-precaching": "^6.5.2"
  },
  "devDependencies": {
    "@types/wicg-file-system-access": "^2023.10.5",
    "i": "^0.3.7",
    "npm": "^10.5.0",
    "typescript": "^5.4.3",
    "vite": "^5.2.2",
    "vite-plugin-pwa": "^0.19.5"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "consistent",
    "trailingComma": "es5",
    "endOfLine": "crlf",
    "bracketSpacing": true
  }
}
